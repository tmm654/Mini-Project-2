class Dog:
    def __init__(self, age, name, is_male, weight):
        self.age = age
        self.name = name
        self.is_male = is_male # Boolean. True if Male, False if Female.
        self.weight = weight
my_dog = Dog(age=1, name="Moe", is_male=False, weight=5)

print("Name:", my_dog.name)
print("Age:", my_dog.age)
print("Is Male:", my_dog.is_male)
print("Weight:", my_dog.weight)
